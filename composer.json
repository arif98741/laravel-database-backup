{
    "name": "mhmdomer/database-backup",
    "description": "Backup your laravel database by a simple artisan command",
    "keywords": [
        "mhmdomer",
        "laravel",
        "database-backup"
    ],
    "homepage": "https://github.com/mhmdomer/database-backup",
    "license": "MIT",
    "authors": [
        {
            "name": "mhmdomer",
            "email": "mohammedomer789@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Mhmdomer\\DatabaseBackup\\": "src",
            "Mhmdomer\\DatabaseBackup\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhmdomer\\DatabaseBackup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhmdomer\\DatabaseBackup\\DatabaseBackupServiceProvider"
            ],
            "aliases": {
                "DatabaseBackup": "Mhmdomer\\DatabaseBackup\\DatabaseBackupFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
